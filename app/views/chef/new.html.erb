<center><h1>Create Canteen Chef Or Food Store Profile</h1>
    <%= form_for :chef do |c| %>
        <p>
        <%= c.label :name %> <br>
        <%= c.text_field :name%>
        </p>
        <p>
        <%= c.label :phone %> <br>
        <%= c.number_field :phone%>
        </p>
        <p>
        <%= c.label :FoodStore%>
        <%= select_tag(:ecompany, options_for_select([['GHI', 1], ['JKL', 2], ['MNO', 3]])) %>
        </p>
        <%= c.submit %>
    <% end %>
</center>
